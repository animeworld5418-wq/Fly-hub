-- Place in StarterPlayerScripts as a LocalScript

local TweenService = game:GetService("TweenService")
local HttpService = game:GetService("HttpService")
local Player = game.Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-------------------------------------------------
-- VALID KEYS (50 STRONG, SINGLE USE)
-------------------------------------------------
local validKeys = {
    "D3v!lN7xP#9Q2rTs", "H3xL@pK2#s8nWq9", "Fz8!kVr1#YpL6mN",
    "U0r!TqX5#eK9vA3", "Zy7!BvD2#QjP6tM", "Lk9!rH8#oE1nS4p",
    "Tt4!bY5#M8qF1vX", "Oa2!mV9#N5lQ7uE", "Wn6!xG3#cT2sL8z",
    "Rr9!uJ1#pY7mD5a", "Eo5!jQ8#lB6nT3v", "N1m!K2#sW5zX8pL",
    "A8!hR4#qP9jD2yN", "G6!fV1#bT3nW7oM", "S3!pY9#kE5qJ2xR",
    "J2!vM8#tB6qP4wH", "C9!xN3#lT5mR7yQ", "B7!zF1#dE2gH4uK",
